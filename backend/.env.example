# Environment Configuration
NODE_ENV=development

# Server Configuration
PORT=2025

# Database Configuration
MONGODB_ATLAS_URL=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority&appName=<appName>

# Cloudinary Configuration (Image Upload Service)
# Sign up at https://cloudinary.com/ to get these values
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_SECRET_KEY=your_secret_key_here

# Frontend Configuration
CLIENT_URL=http://localhost:5173

# JWT Configuration
JWT_SECRET_KEY=your-super-secure-jwt-secret-key-here-minimum-32-characters
JWT_EXPIRY=2d

# Admin Account Configuration
ADMIN_EMAIL=admin@sherising.org
ADMIN_PASSWORD=your_secure_admin_password_here